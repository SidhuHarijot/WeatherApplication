{
	"mb-1": {
		"style": {
			"marginBottom": 4
		}
	},
	"mb-2": {
		"style": {
			"marginBottom": 8
		}
	},
	"mt-4": {
		"style": {
			"marginTop": 16
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"rounded-lg": {
		"style": {
			"borderTopLeftRadius": 8,
			"borderTopRightRadius": 8,
			"borderBottomRightRadius": 8,
			"borderBottomLeftRadius": 8
		}
	},
	"bg-blue-200": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(191 219 254 / var(--tw-bg-opacity))"
		}
	},
	"p-4": {
		"style": {
			"paddingTop": 16,
			"paddingRight": 16,
			"paddingBottom": 16,
			"paddingLeft": 16
		}
	},
	"text-base": {
		"style": {
			"fontSize": 16,
			"lineHeight": 24
		}
	},
	"text-lg": {
		"style": {
			"fontSize": 18,
			"lineHeight": 28
		}
	},
	"text-xl": {
		"style": {
			"fontSize": 20,
			"lineHeight": 28
		}
	},
	"font-bold": {
		"style": {
			"fontWeight": "700"
		}
	},
	"text-red-500": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(239 68 68 / var(--tw-text-opacity))"
		}
	},
	"text-black": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(0 0 0 / var(--tw-text-opacity))"
		}
	}
}